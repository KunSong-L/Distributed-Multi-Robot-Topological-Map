<launch>

  <arg name="model" default="burger"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find fastest_explore)/rviz/robotexplore.rviz" />


  <node name="map_building" pkg="fastest_explore" type="robot_explore.py" output="screen">
    <param name="robot_name" value="robot1"/>
    <param name="robot_num" value="1"/>
  </node>
  
  <!-- data_ana -->
  <node name="exp_data_analysis" pkg="fastest_explore" type="exp_data_analysis.py" output="screen">
    <param name="robot_name" value="robot1"/>
    <param name="robot_num" value="1"/>
    <param name="file_path" value="/home/master/explore_data/exp_data/fastes_exp/large_indoor/"/>
  </node>

</launch>